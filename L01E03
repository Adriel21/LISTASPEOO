#include <stdio.h>
#include <string.h>

int main()
{
	char Sentença[52];
	int c , f , p = -1 , t= 0;
	fgets(Sentença,51, stdin);
	Sentença[strlen(Sentença)-1]='\0';

	for(t; Sentença[t] != '\0'; t++)

	for(c=0, f= t-1 ; f>0 && Sentençac] != '\0' ; c++, f--)
	{
		for(c = c ; Sentença[c]==' ' ; c++)

		for(f = f; Sentença[f]==' ' ; f++)

		if(Sentença[f] != Sentença[c])
		{
			p=0;
			break;
		}
	}
	if(p==0){
		printf("Nao Palindromo\n");
	}
	else{
        printf("Palindromo\n");
    }

	return 0;
}
